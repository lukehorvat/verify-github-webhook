{
  "name": "verify-github-webhook",
  "version": "0.0.0",
  "description": "Safely verify that a received webhook originated from GitHub.",
  "author": "Luke Horvat",
  "license": "MIT",
  "repository": "lukehorvat/verify-github-webhook",
  "main": "dist",
  "scripts": {
    "build": "node_modules/.bin/babel lib -d dist -q",
    "prebuild": "node_modules/.bin/rimraf dist",
    "prepublish": "npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "6.18.0",
    "babel-preset-es2015": "6.18.0",
    "rimraf": "2.5.4"
  },
  "keywords": [
    "github",
    "webhook",
    "webhooks",
    "verify",
    "validate",
    "validator",
    "hash",
    "hex",
    "hmac",
    "signature"
